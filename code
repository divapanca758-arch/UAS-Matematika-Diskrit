class Graph:
    def __init__(self, vertices):
        self.V = vertices
        self.graph = [[0] * self.V for _ in range(self.V)]
    def add_edge(self, u, v):
        self.graph[u][v] = 1
        self.graph[v][u] = 1

class Graph:
    def __init__(self, vertices):
        self.V = vertices
        self.graph = [[]for _ in range(self.V)]
    def add_edge(self, u, v):
        self.graph[u].append(v)
        self.graph[v].append(u)
    
    def dfs(self, v, visited):
        visited[v] = True
        print(v, end=" ")

        for i in self.graph[v]:
            if not visited[i]:
                self.dfs(i, visited)

    def dfs_interative(self, start):
        visited = [False] * self.V
        stack = []

        stack.append(start)
        visited[start] = True

        while stack:
            current = stack.pop()
            print(current, end=" ")

            for neighboor in self.graph[current]:
                if not visited[neighboor]:
                    stack.append(neighboor)
                    visited[neighboor] = True
    
    def bfs(self, start):
        visited = [False] * self.V
        queue = []

        queue.append(start)
        visited[start] =True

        while queue:
            current = queue.pop(0)
            print(current, end=" ")

            for neighboor in self.graph[current]:
                if not visited[neighboor]:
                    queue.append(neighboor)
                    visited[neighboor] = True

g = Graph(4) 
g.add_edge(0, 1) 
g.add_edge(0, 2) 
g.add_edge(1, 2) 
g.add_edge(2, 3) 

print("Depth First Traversal:") 
g.dfs (0, [False]*4) 
print("\nLebar Lintasan Pertama:") 
g.bfs (0)
